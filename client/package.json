{
  "name": "some-schedules",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@fortawesome/fontawesome-pro-webfonts": "^1.0.6",
    "create-react-context": "^0.2.2",
    "date-fns": "^1.29.0",
    "humps": "^2.0.1",
    "jwt-decode": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "styled-components": "^3.2.6",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "build": "NODE_PATH=src/ react-scripts build && mv build/* ../public",
    "eject": "NODE_PATH=src/ react-scripts eject",
    "format":
      "prettier --single-quote --trailing-comma all --arrow-parens always --write **/*.{js,jsx,json}",
    "lint": "NODE_PATH=src/ eslint src/**/*.js*",
    "precommit": "lint-staged",
    "start": "NODE_PATH=src/ react-scripts start",
    "test": "NODE_PATH=src/ react-scripts test --env=jsdom",
    "test:debug":
      "NODE_PATH=src/ react-scripts --inspect-brk test --env=jsdom --runInBand"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --single-quote --trailing-comma all --arrow-parens always --write",
      "git add"
    ]
  }
}
